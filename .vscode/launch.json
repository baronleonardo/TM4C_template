{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/Output.elf",
            "args": ["--init-command=${workspaceRoot}/debug_helper_files/pre", "--command=${workspaceRoot}/debug_helper_files/cmd"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true,
            "miDebuggerServerAddress": "localhost:3333",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/arm-none-eabi-gdb"
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb"
            }
        }
